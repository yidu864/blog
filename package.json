{
  "name": "yidu864-blog",
  "version": "1.0.0",
  "description": "yidu864's blog base on vuepress",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf docs/.vuepress/.temp docs/.vuepress/.cache docs/.vuepress/dist",
    "dev": "vuepress-cli dev docs --clean-cache",
    "build": "pnpm clean && vuepress-cli build docs --clean-cache",
    "serve": "serve docs/.vuepress/dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/yidu864/blog.git"
  },
  "keywords": [
    "blog"
  ],
  "author": "yidu864",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yidu864/blog/issues"
  },
  "homepage": "https://github.com/yidu864/blog#readme",
  "devDependencies": {
    "@algolia/client-search": "^4.14.3",
    "@vuepress/bundler-vite": "2.0.0-beta.49",
    "@vuepress/cli": "2.0.0-beta.49",
    "@vuepress/client": "2.0.0-beta.49",
    "@vuepress/plugin-docsearch": "2.0.0-beta.60",
    "oh-vue-icons": "^1.0.0-rc3",
    "rimraf": "^3.0.2",
    "sass-loader": "^13.0.2",
    "serve": "^14.0.1",
    "vue": "^3.2.36",
    "vuepress": "2.0.0-beta.49",
    "vuepress-theme-gungnir": "2.0.0-alpha.26"
  },
  "engines": {
    "node": ">=14"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack"
      ]
    }
  }
}
